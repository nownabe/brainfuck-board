{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier",
    "plugin:import/recommended",
    "plugin:import/warnings"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": { "order": "asc" }
      }
    ],
    "react/jsx-sort-props": ["error", { "reservedFirst": true }]
  }
}
